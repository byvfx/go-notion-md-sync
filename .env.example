# Copy this file to .env and fill in your actual values
# Get your integration token from: https://www.notion.so/my-integrations

# Your Notion Integration Token
NOTION_MD_SYNC_NOTION_TOKEN=your_notion_integration_token_here

# Your Notion Parent Page ID (32-character hex string from page URL)
NOTION_MD_SYNC_NOTION_PARENT_PAGE_ID=your_parent_page_id_here

# Optional: Override config.yaml settings via environment variables
# NOTION_MD_SYNC_DIRECTORIES_MARKDOWN_ROOT=./docs
# NOTION_MD_SYNC_SYNC_DIRECTION=push
# NOTION_MD_SYNC_SYNC_CONFLICT_RESOLUTION=newer
# NOTION_MD_SYNC_PERFORMANCE_WORKERS=30
